{
	"extends": [
		"config:recommended"
	],
	"rangeStrategy": "bump",
	"packageRules": [
		{
			"matchUpdateTypes": [
				"minor",
				"patch",
				"pin",
				"digest",
				"major"
			]
		}
	],
	"commitMessagePrefix": "⬆️",
	"prConcurrentLimit": 0,
	"prHourlyLimit": 0,
	"dependencyDashboard": true
}
